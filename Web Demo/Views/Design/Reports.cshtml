@using Stimulsoft.Report.MvcMobile;

@{
    ViewBag.Title = "Stimulsoft Reports.Web Demo";
}

@Html.Stimulsoft().StiMvcMobileDesigner("StiMvcMobileDesigner1", new StiMvcMobileDesignerOptions()
